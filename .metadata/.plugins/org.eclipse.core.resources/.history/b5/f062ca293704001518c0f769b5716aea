package andrew.deng.multithreading;

import java.util.concurrent.RecursiveAction;

public class ParallelArrayInitializer {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

	public static void parallelAssignValues(double[] list) {

	}

	private static class paralleAssignValueTask extends RecursiveAction {

		private final int SUBTASKNUM;

		private int list[];

		public paralleAssignValueTask(int[] list, int subTaskNum) {
			super();
			this.list = list;
			this.SUBTASKNUM = subTaskNum;
		}

		@Override
		protected void compute() {
			int[] firstHalf = new int[list.length / 2];
			System.arraycopy(list, 0, firstHalf, 0, list.length / 2);

		}

	}

}
